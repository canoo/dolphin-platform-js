{
  "name": "dolphin-platform-js",
  "version": "0.18.0",
  "description": "JavaScript client of the Dolphin Platform",
  "main": "./dist/dolphin-platform.min.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/canoo/dolphin-platform-js.git"
  },
  "scripts": {
    "clean": "node ./node_modules/rimraf/bin.js dist/",
    "build": "node ./node_modules/webpack/bin/webpack.js",
    "build:install": "npm install && node ./node_modules/bower/bin/bower install && npm run build",
    "test": "npm run test:mocha",
    "test:mocha": "node ./node_modules/mocha/bin/mocha test/src --require babel-register",
    "test:nyc": "node ./node_modules/nyc/bin/nyc.js --require babel-register --reporter=html --reporter=text mocha test/src"
  },
  "author": "Michael Heinrichs <michael.heinrichs@canoo.com>",
  "license": "Apache-2.0",
  "devDependencies": {
    "@types/chai": "4.1.0",
    "@types/mocha": "2.2.46",
    "babel": "6.23.0",
    "babel-core": "6.26.0",
    "babel-plugin-istanbul": "4.1.5",
    "babel-plugin-transform-object-rest-spread": "6.26.0",
    "babel-preset-es2015": "6.24.1",
    "babel-register": "6.26.0",
    "bower": "1.8.2",
    "chai": "4.1.2",
    "eslint": "4.15.0",
    "istanbul": "0.4.5",
    "mocha": "4.1.0",
    "nyc": "11.4.1",
    "rimraf": "2.6.2",
    "sinon": "1.17.x",
    "uglifyjs-webpack-plugin": "1.1.6",
    "webpack": "3.10.0"
  },
  "dependencies": {
    "emitter-component": "1.1.1"
  }
}
